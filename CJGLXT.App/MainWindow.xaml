<Window x:Class="CJGLXT.App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CJGLXT.App"
        mc:Ignorable="d"
        Title="MainWindow" Height="750" Width="800">
    <Grid VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <DockPanel>
            <StackPanel DockPanel.Dock="Top"  VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                <TextBlock x:Name="textblock" Text="Test" HorizontalAlignment="Stretch"></TextBlock>
                <Button Content="Test" Click="ButtonBase_OnClick"></Button>
                <Button Click="ButtonBase_OnClick1">Navgate</Button>
                <TextBox Text="{Binding Test.Oj2.Oj3.Text,Mode=TwoWay}"></TextBox>
                <TextBlock x:Name="Result">AAAA</TextBlock>
                <Button  Click="ChangeRef">Change Reference</Button>
                <Button Click="LoadText">Load</Button>
            </StackPanel>
            <Frame x:Name="frame" DockPanel.Dock="Bottom" Background="GreenYellow" VerticalAlignment="Stretch"  NavigationUIVisibility="Visible"></Frame>
        </DockPanel>
    </Grid>
</Window>
